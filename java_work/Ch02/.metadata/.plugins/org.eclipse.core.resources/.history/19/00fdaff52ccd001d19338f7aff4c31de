package Ch02_;

import java.util.Scanner;

public class ch02_ex04 {

	public static void main(String[] args) {
		char grade;
		Scanner scanner = new Scanner(System.in);
		
		System.out.print("점수(0~100):");
		 int score = scanner.nextInt();
		 if(score>=90)
			 grade='A';
		 else if(score>=80)
			 grade='B';
		 else if(score>=70)
			 grade='C';
		 else if(score>=60)
			 grade='D';
		 else(score>=50)
			 grade='F';
			System.out.print("학점은"+grade+"용");
		 scanner.close();

	}

}
